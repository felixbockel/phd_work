## prostate without motion ##

# loop for print out ###

List2=list()
List3=list()
for(i in loop_seq){
  aaa=(i/(max(loop_seq)+1))
  List1=list()
  for(ii in 1:ncol(matrix_prostate_0shift)){
    a=matrix_prostate_0shift[,ii]
    b=a [! a %in% remove]
    LM1=which(abs(b-aaa)==min(abs(b-aaa)))
    List1[[length(List1)+1]] = LM1
  }
  d <- as.numeric(unlist(List1))
 
  List1=list()
  for(ii in 1:length(d)){
    a=d[ii]
    b=a [! a %in% remove]
    LM1=dose[c(b)]
    List1[[length(List1)+1]] = LM1
  }
  d_dose <- as.numeric(unlist(List1))
  
  LM2=round(sd(d_dose),1)
  LM3=round(mean(d_dose),1)
  List2[[length(List2)+1]] = LM2
  List3[[length(List3)+1]] = LM3
}
prostate_0_sd <- as.numeric(unlist(List2))
prostate_0_mean <- as.numeric(unlist(List3))

# fill with zeros
min_pros_mean=min(prostate_0_mean)
prost_mean_seq=seq(0,min_pros_mean,0.5) 
length_zeros=length(prost_mean_seq)
zero_vector=rep(0,length_zeros)
length_hundreds=rep(100,length_zeros)

#dmax
List1=list()
for(i in 1:ncol(matrix_prostate_0shift)){
  a=matrix_prostate_0shift[,i]
  b=a [! a %in% remove]
  bb=b[length(b)]
  LM1=max(which(b ==bb))
  List1[[length(List1)+1]] = LM1
 }
d <- as.numeric(unlist(List1))

List1=list()
for(i in 1:length(d)){
  a=d[i]
  b=a [! a %in% remove]
  LM1=dose[c(b)]
  List1[[length(List1)+1]] = LM1
 }
d_dose <- as.numeric(unlist(List1))

prostate_0_sd_dmax=round(sd(d_dose),1)
prostate_0_mean_dmax=round(mean(d_dose),1)

mean_prostate_0shift=c(prost_mean_seq,prostate_0_mean,prostate_0_mean_dmax)
sd_prostate_0shift=c(zero_vector,prostate_0_sd,prostate_0_sd_dmax)
