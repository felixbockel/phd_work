## PTV coverage ##

# initial ptv ###

# loop for print out ###

loop_seq=rev(seq(1,99,2)) # 
#print(loop_seq)
#print(length(loop_seq))

List2=list()
List3=list()
for(i in loop_seq){
  aaa=(i/(max(loop_seq)+1))
  List1=list()
  for(ii in 1:ncol(matrix_prostate_shift)){
    a=matrix_prostate_shift[,ii]
    b=a [! a %in% remove]
    LM1=which(abs(b-aaa)==min(abs(b-aaa)))
    List1[[length(List1)+1]] = LM1
  }
  d <- as.numeric(unlist(List1))
 
  List1=list()
  for(ii in 1:length(d)){
    a=d[ii]
    b=a [! a %in% remove]
    LM1=dose[c(b)]
    List1[[length(List1)+1]] = LM1
  }
  d_dose <- as.numeric(unlist(List1))
  
  LM2=round(sd(d_dose),1)
  LM3=round(mean(d_dose),1)
  List2[[length(List2)+1]] = LM2
  List3[[length(List3)+1]] = LM3
}
prostate_sd <- as.numeric(unlist(List2))
prostate_mean <- as.numeric(unlist(List3))

# fill with zeros
min_pros_mean=min(prostate_mean)
prost_mean_seq=seq(0,min_pros_mean,0.5) 
length_zeros=length(prost_mean_seq)
zero_vector=rep(0,length_zeros)
length_hundreds=rep(100,length_zeros)

