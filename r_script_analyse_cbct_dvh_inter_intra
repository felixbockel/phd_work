#d05
your.number=0.05 # d5

List1=list()
for(i in 1:ncol(matrix_prostate_shift)){
  a=matrix_prostate_shift[,i]
  b=a [! a %in% remove]
  LM1=which(abs(b-your.number)==min(abs(b-your.number)))
  List1[[length(List1)+1]] = LM1
 }
d <- as.numeric(unlist(List1))

List1=list()
for(i in 1:length(d)){
  a=d[i]
  b=a [! a %in% remove]
  LM1=dose[c(b)]
  List1[[length(List1)+1]] = LM1
 }
d_dose <- as.numeric(unlist(List1))

prostate_sd_05=round(sd(d_dose),1)
prostate_mean_05=round(mean(d_dose),1)

