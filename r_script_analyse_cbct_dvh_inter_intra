plot(mean_prostate_0shift,x_seq_pro_0, type=type_line_nonshift,  lty=lty_solid,col=col_gray_dark,ylab=NA, xlab=NA,yaxt="n",xaxt = "n")#ylab=NA, xlab=NA,yaxt="n",xaxt = "n",
lines(prostate_shift_dose_mean, seq_prostate,type=type_line_shift,  lty=lty_dotted,col=col_gray_dark)
lines(mean_rektwand_0shift,x_seq_rektwand_0,type=type_line_shift,  lty=lty_solid,col=col_gray_light)
lines(mean_rektwand_shift,x_seq_rektwand,type=type_line_shift,  lty=lty_dotted,col=col_gray_light)
lines(mean_rektum_0shift,x_seq_rektum_0,type=type_line_shift,  lty=lty_solid,col=col_gray_very_light)
lines(mean_rektum_shift,x_seq_rektum,type=type_line_shift,  lty=lty_dotted,col=col_gray_very_light)
arrows(min_mean_prostate_0shift,x_seq_pro_0shift_cut,max_mean_prostate_0shift,x_seq_pro_0shift_cut, code=3, length=0.01, angle = 90, col=col_gray_dark, lty=lty_solid)# length= gre der linien
