##### print and calc pdf7 ######
# calc
step_size=6
start_offset=3
# prostate 0shift
List1=list()
List2=list()
for(i in 1:length(mean_prostate_0shift)){
  a=mean_prostate_0shift[i] 
  b=prostate_shift_dose_sd[i] # swapped sd
  LM1=a-b 
  LM2=a+b
  LM3=x_seq_pro_0[i]
  List1[[length(List1)+1]] = LM1
  List2[[length(List2)+1]] = LM2
 }
min_mean_prostate_0shift <- as.numeric(unlist(List1))
max_mean_prostate_0shift <- as.numeric(unlist(List2))
min_mean_prostate_0shift <- min_mean_prostate_0shift[seq(1,length(min_mean_prostate_0shift),step_size)]
max_mean_prostate_0shift <- max_mean_prostate_0shift[seq(1,length(max_mean_prostate_0shift),step_size)]
x_seq_pro_0shift_cut <- x_seq_pro_0[seq(1,length(x_seq_pro_0),step_size)]

# prostate shift
List1=list()
List2=list()
for(i in 1:length(prostate_shift_dose_mean)){
  a=prostate_shift_dose_mean[i]
  b=sd_prostate_0shift[i] # swapped sd
  LM1=a-b 
  LM2=a+b
  List1[[length(List1)+1]] = LM1
  List2[[length(List2)+1]] = LM2
 }
min_mean_prostate_shift <- as.numeric(unlist(List1))
max_mean_prostate_shift <- as.numeric(unlist(List2))
min_mean_prostate_shift <- min_mean_prostate_shift[seq(start_offset,length(min_mean_prostate_shift),step_size)]
max_mean_prostate_shift <- max_mean_prostate_shift[seq(start_offset,length(max_mean_prostate_shift),step_size)]
x_seq_pro_shift_cut <- seq_prostate[seq(start_offset,length(seq_prostate),step_size)]

# rektumwall 0shift

List1=list()
List2=list()
for(i in 1:length(mean_rektwand_0shift)){
  a=mean_rektwand_0shift[i]
  b=sd_rektwand_shift[i] # swapped sd
  LM1=a-b 
  LM2=a+b
  List1[[length(List1)+1]] = LM1
  List2[[length(List2)+1]] = LM2
 }
min_mean_rektwand_0shift <- as.numeric(unlist(List1))
max_mean_rektwand_0shift <- as.numeric(unlist(List2))
min_mean_rektwand_0shift <- min_mean_rektwand_0shift[seq(1,length(min_mean_rektwand_0shift),step_size)]
max_mean_rektwand_0shift <- max_mean_rektwand_0shift[seq(1,length(max_mean_rektwand_0shift),step_size)]
x_seq_rektwand_0shift_cut <- x_seq_rektwand_0[seq(1,length(x_seq_rektwand_0),step_size)]

# rektumwall shift

List1=list()
List2=list()
for(i in 1:length(mean_rektwand_shift)){
  a=mean_rektwand_shift[i]
  b=sd_rektwand_0shift[i] # swapped sd
  LM1=a-b 
  LM2=a+b
  List1[[length(List1)+1]] = LM1
  List2[[length(List2)+1]] = LM2
 }
min_mean_rektwand_shift <- as.numeric(unlist(List1))
max_mean_rektwand_shift <- as.numeric(unlist(List2))
min_mean_rektwand_shift <- min_mean_rektwand_shift[seq(start_offset,length(min_mean_rektwand_shift),step_size)]
max_mean_rektwand_shift <- max_mean_rektwand_shift[seq(start_offset,length(max_mean_rektwand_shift),step_size)]
x_seq_rektwand_shift_cut <- x_seq_rektwand[seq(start_offset,length(x_seq_rektwand),step_size)]

# rektum 0shift

List1=list()
List2=list()
for(i in 1:length(mean_rektum_0shift)){
  a=mean_rektum_0shift[i]
  b=sd_rektum_shift[i] # swapped sd
  LM1=a-b 
  LM2=a+b
  List1[[length(List1)+1]] = LM1
  List2[[length(List2)+1]] = LM2
 }
min_mean_rekt_0shift <- as.numeric(unlist(List1))
max_mean_rekt_0shift <- as.numeric(unlist(List2))
min_mean_rekt_0shift <- min_mean_rekt_0shift[seq(1,length(min_mean_rekt_0shift),step_size)]
max_mean_rekt_0shift <- max_mean_rekt_0shift[seq(1,length(max_mean_rekt_0shift),step_size)]
x_seq_rektum_0_cut <- x_seq_rektum_0[seq(1,length(x_seq_rektum_0),step_size)]

# rektum shift

List1=list()
List2=list()
for(i in 1:length(mean_rektum_shift)){
  a=mean_rektum_shift[i]
  b=sd_rektum_0shift[i] # swapped sd
  LM1=a-b 
  LM2=a+b
  List1[[length(List1)+1]] = LM1
  List2[[length(List2)+1]] = LM2
 }
min_mean_rekt_shift <- as.numeric(unlist(List1))
max_mean_rekt_shift <- as.numeric(unlist(List2))
min_mean_rekt_shift <- min_mean_rekt_shift[seq(start_offset,length(min_mean_rekt_shift),step_size)]
max_mean_rekt_shift <- max_mean_rekt_shift[seq(start_offset,length(max_mean_rekt_shift),step_size)]
x_seq_rektum_cut <- x_seq_rektum[seq(start_offset,length(x_seq_rektum),step_size)]


