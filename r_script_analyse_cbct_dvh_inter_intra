List1=list()
List2=list()
for(i in 1:nrow(matrix_prostate_0shift)){
  a=matrix_prostate_0shift[i,]
  b=a [! a %in% remove]
  LM1=sd(b)
  LM2=mean(b)
  List1[[length(List1)+1]] = LM1
  List2[[length(List2)+1]] = LM2
 }
sd_prostate_0shift <- as.numeric(unlist(List1))
mean_prostate_0shift <- as.numeric(unlist(List2))

