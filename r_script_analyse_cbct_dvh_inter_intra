arrows(min_mean_rekt_shift,x_seq_rektum_cut,max_mean_rekt_shift,x_seq_rektum_cut, code=3, length=0.01, angle = 90, col=col_gray_very_light, lty=lty_dotted)# length= gre der linien
box()
axis(1, at=seq_dose, las=1,labels= seq_dose)
