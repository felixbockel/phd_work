## Dmean Bulb

# initial ptv
matrix_dmean=rbind(dmean_1_int,dmean_2_int,dmean_4_int,dmean_5_int,dmean_7_int,dmean_8_int,dmean_10_int,dmean_12_int,dmean_14_int,dmean_28_int,dmean_44_int,dmean_45_int,dmean_46_int,dmean_49_int,dmean_53_int,dmean_2_int,dmean_56_int,dmean_62_int)
mean_dmean_bulb_int=mean(matrix_dmean[,1])
sd_dmean_bulb_int=sd(matrix_dmean[,1])
bulb_mean_int=matrix_dmean
# adopted ptv
#mean_dmean_bulb_ado=(sum(dmean_1_ado,dmean_2_ado,dmean_4_ado,dmean_5_ado,dmean_7_ado,dmean_8_ado,dmean_10_ado,dmean_12_ado,dmean_14_ado,dmean_28_ado,dmean_44_ado,dmean_45_ado,dmean_46_ado,dmean_49_ado,dmean_53_ado,dmean_2_ado,dmean_56_ado,dmean_62_ado))/17
matrix_dmean=rbind(dmean_1_ado,dmean_2_ado,dmean_4_ado,dmean_5_ado,dmean_7_ado,dmean_8_ado,dmean_10_ado,dmean_12_ado,dmean_14_ado,dmean_28_ado,dmean_44_ado,dmean_45_ado,dmean_46_ado,dmean_49_ado,dmean_53_ado,dmean_2_ado,dmean_56_ado,dmean_62_ado)
mean_dmean_bulb_ado=mean(matrix_dmean[,1])
sd_dmean_bulb_ado=sd(matrix_dmean[,1])
bulb_mean_ado=matrix_dmean
## Bulb dose: D30,40,50,60,70,75,90,Dmax -> Dose which has received 30%... of the volume


# Bulb 0 shift -- initial ptv ##

dose=x_seq_bulb_0_ld

# loop for print out ###

loop_seq=rev(seq(1,99,2)) # 
#print(loop_seq)
#print(length(loop_seq))

List2=list()
List3=list()
for(i in loop_seq){
  aaa=(i/(max(loop_seq)+1))
  List1=list()
  for(ii in 1:ncol(matrix_bulb_0shift)){
    a=matrix_bulb_0shift[,ii]
    b=a [! a %in% remove]
    LM1=which(abs(b-aaa)==min(abs(b-aaa)))
    List1[[length(List1)+1]] = LM1
  }
  d <- as.numeric(unlist(List1))
 
  List1=list()
  for(ii in 1:length(d)){
    a=d[ii]
    b=a [! a %in% remove]
    LM1=dose[c(b)]
    List1[[length(List1)+1]] = LM1
  }
  d_dose <- as.numeric(unlist(List1))
  
  LM2=round(sd(d_dose),1)
  LM3=round(mean(d_dose),1)
  List2[[length(List2)+1]] = LM2
  List3[[length(List3)+1]] = LM3
}
bulb_sd <- as.numeric(unlist(List2))
bulb_mean <- as.numeric(unlist(List3))

# fill with zeros
min_bulb_mean=min(bulb_mean)
bulb_mean_seq=seq(0,min_bulb_mean,0.5) 
length_zeros=length(bulb_mean_seq)
zero_vector=rep(0,length_zeros)
length_hundreds=rep(100,length_zeros)


#d05
your.number=0.05 # d5

List1=list()
for(i in 1:ncol(matrix_bulb_0shift)){
  a=matrix_bulb_0shift[,i]
  b=a [! a %in% remove]
  LM1=which(abs(b-your.number)==min(abs(b-your.number)))
  List1[[length(List1)+1]] = LM1
 }
d <- as.numeric(unlist(List1))

List1=list()
for(i in 1:length(d)){
  a=d[i]
  b=a [! a %in% remove]
  LM1=dose[c(b)]
  List1[[length(List1)+1]] = LM1
 }
d_dose <- as.numeric(unlist(List1))

bulb_sd_05=round(sd(d_dose),1)
bulb_mean_05=round(mean(d_dose),1)

#d10
your.number=0.10 # d10

List1=list()
for(i in 1:ncol(matrix_bulb_0shift)){
  a=matrix_bulb_0shift[,i]
  b=a [! a %in% remove]
  LM1=which(abs(b-your.number)==min(abs(b-your.number)))
  List1[[length(List1)+1]] = LM1
 }
d <- as.numeric(unlist(List1))

List1=list()
for(i in 1:length(d)){
  a=d[i]
  b=a [! a %in% remove]
  LM1=dose[c(b)]
  List1[[length(List1)+1]] = LM1
 }
d_dose <- as.numeric(unlist(List1))

bulb_sd_10=round(sd(d_dose),1)
bulb_mean_10=round(mean(d_dose),1)

