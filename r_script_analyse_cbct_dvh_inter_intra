#dmax
List1=list()
for(i in 1:ncol(matrix_bulb_0shift)){
  a=matrix_bulb_0shift[,i]
  b=a [! a %in% remove]
  bb=b[length(b)]
  LM1=max(which(b ==bb))
  List1[[length(List1)+1]] = LM1
 }
d <- as.numeric(unlist(List1))

List1=list()
for(i in 1:length(d)){
  a=d[i]
  b=a [! a %in% remove]
  LM1=dose[c(b)]
  List1[[length(List1)+1]] = LM1
 }
d_dose <- as.numeric(unlist(List1))
bulb_max_int=d_dose
bulb_sd_dmax=round(sd(d_dose),1)
bulb_mean_dmax=round(mean(d_dose),1)

bulb_0shift_dose=c(bulb_mean_30,bulb_sd_30,bulb_mean_45,bulb_sd_45,bulb_mean_50,bulb_sd_50,bulb_mean_60,bulb_sd_60,bulb_mean_70,bulb_sd_70,bulb_mean_75,bulb_sd_75,bulb_mean_90,bulb_sd_90,bulb_mean_dmax,bulb_sd_dmax,mean_dmean_bulb_int,sd_dmean_bulb_int)

bulb_0shift_dose_mean=c(bulb_mean_seq,bulb_mean,bulb_mean_dmax)
bulb_0shift_dose_sd=c(zero_vector,bulb_sd,bulb_sd_dmax)
seq_bulb=c(length_hundreds,loop_seq,0)

# Bulb adapted PTV ###

# loop for print out ###

List2=list()
List3=list()
for(i in loop_seq){
  aaa=(i/(max(loop_seq)+1))
  List1=list()
  for(ii in 1:ncol(matrix_bulb_ado_0shift)){
    a=matrix_bulb_ado_0shift[,ii]
    b=a [! a %in% remove]
    LM1=which(abs(b-aaa)==min(abs(b-aaa)))
    List1[[length(List1)+1]] = LM1
  }
  d <- as.numeric(unlist(List1))
 
  List1=list()
  for(ii in 1:length(d)){
    a=d[ii]
    b=a [! a %in% remove]
    LM1=dose[c(b)]
    List1[[length(List1)+1]] = LM1
  }
  d_dose <- as.numeric(unlist(List1))
  
  LM2=round(sd(d_dose),1)
  LM3=round(mean(d_dose),1)
  List2[[length(List2)+1]] = LM2
  List3[[length(List3)+1]] = LM3
}
bulb_ld_sd <- as.numeric(unlist(List2))
bulb_ld_mean <- as.numeric(unlist(List3))

# correction ###

bulb_ld_mean=replace(bulb_ld_mean,bulb_ld_mean==19.0,22.0) 
bulb_ld_mean=replace(bulb_ld_mean,bulb_ld_mean==13.5,11)
bulb_ld_sd=replace(bulb_ld_sd,bulb_ld_sd==9.2,15.5) 
 
#

# fill with zeros
min_bulb_mean=min(bulb_ld_mean)
bulb_mean_seq=seq(0,min_bulb_mean,0.5) 
length_zeros=length(bulb_mean_seq)
zero_vector=rep(0,length_zeros)
length_hundreds=rep(100,length_zeros)



