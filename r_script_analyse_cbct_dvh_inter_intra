arrows(min_mean_rektwand_shift,x_seq_rektwand_shift_cut,max_mean_rektwand_shift,x_seq_rektwand_shift_cut, code=3, length=0.01, angle = 90, col=col_gray_light, lty=lty_dotted)# length= gre der linien
