## p and t - value tests ## 1-p value new assumption 0.05

#p value as input (assumption) is alpha -> the conf.level. is equivalent to '1 - alpha' -> default for t.test is conf.level 0.95 

# p<0.05 considered significant correlation between x and y. p>0.05 the opposite: kk Wang 2010
# p<0.001 high correlation :jones 2013
# t:  t kann sowohl negativ als auch positiv sein ? negativ bedeutet, dass der Mittelwert x kleiner als Mittelwert y unseres Versuchs war; positiv entsprechend anders herum- http://scienceblogs.de/andererseits/2010/08/17/statistik-fur-anfanger-der-ttest/
# p<0.01 high correlation : bassim paper 1% unterschied
# for inter-fractional set-up variability versus treatment days ##

# Inter-fractional marker motion: inital vs replanning

# bulb

# bulb D30
value_x=bulb_30_int  
value_y=bulb_30_ado
stat_rtdays=t.test(value_x,value_y)
p_bulb_30=stat_rtdays$p.value

# bulb D45
value_x=bulb_45_int  
value_y=bulb_45_ado
stat_rtdays=t.test(value_x,value_y)
p_bulb_45=stat_rtdays$p.value

# bulb D50
value_x=bulb_50_int  
value_y=bulb_50_ado
stat_rtdays=t.test(value_x,value_y)
p_bulb_50=stat_rtdays$p.value

# bulb D60
value_x=bulb_60_int  
value_y=bulb_60_ado
stat_rtdays=t.test(value_x,value_y)
p_bulb_60=stat_rtdays$p.value

# bulb D70
value_x=bulb_70_int  
value_y=bulb_70_ado
stat_rtdays=t.test(value_x,value_y)
p_bulb_70=stat_rtdays$p.value

# bulb D75
value_x=bulb_75_int  
value_y=bulb_75_ado
stat_rtdays=t.test(value_x,value_y)
p_bulb_75=stat_rtdays$p.value

# bulb D90
value_x=bulb_90_int  
value_y=bulb_90_ado
stat_rtdays=t.test(value_x,value_y)
p_bulb_90=stat_rtdays$p.value

# bulb Dmax
value_x=bulb_max_int 
value_y=bulb_max_ado
stat_rtdays=t.test(value_x,value_y)
p_bulb_max=stat_rtdays$p.value

# bulb Dmean
value_x=bulb_mean_int  
value_y=bulb_mean_ado
stat_rtdays=t.test(value_x,value_y)
p_bulb_mean=stat_rtdays$p.value

bulb_p_all=c(p_bulb_30,p_bulb_45,p_bulb_50,p_bulb_60,p_bulb_70,p_bulb_75,p_bulb_90,p_bulb_max,p_bulb_mean)

# prostate shift

value_x=mean_prostate_0shift  
value_y=prostate_shift_dose_mean
stat_rtdays=t.test(value_x,value_y)
ttest_irplan_marker_x=stat_rtdays$statistic
p_prostate=ifelse((stat_rtdays$p.value)<=0.05,0.05,round(stat_rtdays$p.value,2))# p_rtdays<0.001 -> result equals p<0.001 otherwise p>0.001

# rektumwand shift

value_x=mean_rektwand_0shift 
value_y=mean_rektwand_shift
stat_rtdays=t.test(value_x,value_y)
ttest_irplan_marker_x=stat_rtdays$statistic
p_rektumwand=ifelse((stat_rtdays$p.value)<=0.05,0.05,round(stat_rtdays$p.value,2))# p_rtdays<0.001 -> result equals p<0.001 otherwise p>0.001

# rektum shift

value_x=mean_rektum_0shift  
value_y=mean_rektum_shift
stat_rtdays=t.test(value_x,value_y)
ttest_irplan_marker_x=stat_rtdays$statistic
p_rektum=ifelse((stat_rtdays$p.value)<=0.05,0.05,round(stat_rtdays$p.value,2))# p_rtdays<0.001 -> result equals p<0.001 otherwise p>0.001

# PTV normal and smaller size shift

value_x=mean_prostate_shift # PTV normal size
value_y=mean_prostate_shift_small # PTV smaller size
stat_rtdays=t.test(value_x,value_y)
ttest_irplan_marker_x=stat_rtdays$statistic
p_ptv_small_big=ifelse((stat_rtdays$p.value)<=0.05,0.05,round(stat_rtdays$p.value,2))# p_rtdays<0.001 -> result equals p<0.001 otherwise p>0.001
# bulb: bulb with normal dose and less dose 

value_x=mean_bulb_0shift 
value_y=mean_bulb_0shift_ld
stat_rtdays=t.test(value_x,value_y)
ttest_irplan_marker_x=stat_rtdays$statistic
p_bulb_nd_ld=ifelse((stat_rtdays$p.value)<=0.05,0.05,round(stat_rtdays$p.value,2))# p_rtdays<0.001 -> result equals p<0.001 otherwise p>0.001

## function for hist y-axis rounding ##

RoundUp <- function(from,to){ceiling(from/to)*to}

#
# max_seq_sd=max(c(sd_prostate_0shift_kNA<-sd_prostate_0shift [! sd_prostate_0shift %in% remove],sd_prostate_shift_kNA<-sd_prostate_shift [! sd_prostate_shift %in% remove],
# sd_rektwand_shift_kNA<-sd_rektwand_shift [! sd_rektwand_shift %in% remove],sd_rektwand_shift_kNA<-sd_rektwand_shift [! sd_rektwand_shift %in% remove],
# sd_rektum_shift_kNA<-sd_rektum_shift [! sd_rektum_shift %in% remove],sd_rektum_shift_kNA<-sd_rektum_shift [! sd_rektum_shift %in% remove]),
# sd_bulb_0shift_kNA<-sd_bulb_0shift [! sd_bulb_0shift %in% remove],shift_ld_kNA<-sd_bulb_0shift_ld [! sd_bulb_0shift_ld %in% remove],
# sd_prostate_shift_small_kNA<-sd_prostate_shift_small [! sd_prostate_shift_small %in% remove])

max_seq_sd=max(c(sd_prostate_0shift,prostate_shift_dose_sd,sd_rektwand_0shift,sd_rektwand_shift,sd_rektum_0shift,sd_rektum_shift))


labels_y=seq(0,RoundUp(max_seq_sd,5),5)
#

max_seq_sd2=max(c(bulb_0shift_ld_dose_sd,bulb_0shift_dose_sd))
labels_y2=seq(0,RoundUp(max_seq_sd2,10),5)
#
max_seq_sd3=max(c(prostate_shift_dose_sd,prostate_ado_shift_dose_sd))
labels_y3=seq(0,RoundUp(max_seq_sd3,5),1)


