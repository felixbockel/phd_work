#!/bin/bash

####### input ########
Path="/home/boeckefx/Documents/Prostate_SOP/Data/dvh_kontur_shift/Ammon_Klaus/Dose_Kontur_Shift" 
PLASTIMATCH="/projects/Plastimatch/Plastimatch/plastimatch"
MULTIPLY="/projects/3DSlicer/3DSlicer/Slicer-build/lib/Slicer-4.5/cli-modules/MultiplyScalarVolumes"
a="0"
ROIMean="/home/boeckefx/Documents/Prostate_SOP/scripte/ROIMean/ROIMean-build/ROIMean"
kontur_folder_analysis="/home/boeckefx/Documents/Prostate_SOP/Data/dvh_kontur_shift/analysis/"
## prepare data before running run_all.sh script


# plastimatch convert --input /home/dicom-in/BrauerINT_Reinhard_20161122_105658790/RD.1.2.276.0.20.1.0.5.320293131756.87888.1479809447.186276 --output-dose-img /home/boeckefx/Documents/Prostate_SOP/Data/dvh_kontur_shift/BrauerINT/Dose.nrrd
# plastimatch convert --input /home/dicom-in/Ammon_Klaus_20161122_104314088/RS.1.2.276.0.20.1.4.5.320293131756.87888.1479808563.581012 --output-prefix /home/boeckefx/Documents/Prostate_SOP/Data/dvh_kontur_shift/Ammon/Contours --prefix-format nrrd
# plastimatch convert --input /home/dicom-in/BrauerINT_Reinhard_20161122_105658790/ --output-img /home/boeckefx/Documents/Prostate_SOP/Data/dvh_kontur_shift/BrauerINT/CT_Brauer.nrrd
# for i in /home/boeckefx/Documents/Prostate_SOP/Data/dvh_kontur_shift/Ammon/Contours/*; do mv "$i" $(echo "$i"| sed 's/ /_/g'); done ## alle leerzeichen vom kontur namen sind weg


## Aufruf: ./run_all.sh -f Ammon/balloon_shift.txt -fm Ammon/marker_shift.txt -fmi Ammon/marker_intrafra_shift.txt -ii intra -d Ammon/Dose.nrrd -df 0.0476190 -ent1 Rektum -ent2 Rektumvorderwand -ent3 Prostate -ent4 PTV -ent5 Bulb

#filename="Ammon_Klaus/balloon_shift.txt"

## for balloon_shift.txt- which contours?
# Rektum and Rektumvorderwand

## for marker_markerintra.txt- which contours?
# Prostate

dose_fraktionation="0.0357142" # -> 1/28 ist Standard -> 1/21 = 0.0476190 ... (für ammon)
entity1="Rektum";
entity2="Rektumvorderwand";
entity3="Prostate";
entity4="PTV"
entity5="Bulb"
inter_intra="intra"; # either inter oder inter+ intra shift will be analyzed; inter+ intra shift is standard parameter

while [ $1 ]; do
    case $1 in
        "-f")         filename=$2;; # filename of shift: balloon_shift.txt oder marker_markerintra.txt 
        
        "-fm")        filename1=$2;; # filename marker_shift  -> 
        
        "-fmi")       filename2=$2;; # filename marker_intrafra_shift
        
        "-ii")        inter_intra=$2;; # inter or intra
	
	"-d")         dose=$2;; # dose.nrrd

        "-df")        dose_fraktionation=$2;; # -> 1/28 ist Standard -> 1/21 = 0.0476190 ... (für ammon)
        
        "-ent1")      entity1=$2;; # Rektum 

        "-ent2")      entity2=$2;; # Rektumvorderwand
        
        "-ent3")      entity3=$2;; # Prostate 
        
        "-ent4")      entity4=$2;; # PTV 
        
        "-ent5")      entity5=$2;; # Bulb
                
    esac
    shift
done

FileName=${filename1%/*}
filename_ballon_shift_final="$FileName/balloon_shift_final.txt"

# balloon_shift.txt to switch x,z,y to x,y,z and multiply x,y with -1 --> contour stays at the same location and dose moves therefore x,y shifts needs to be opposite

cat $filename | awk '{printf( "%.2f\t%.2f\t%.2f\n", $1 * (-1), $2, $3 * (-1) )}' > $filename_ballon_shift_final

# calculating marker_shift.txt + marker_intrafra_shift.txt -> interfrac + intrafrac shift as worst case shift for each fraction

if [ "$inter_intra" == "intra" ]; then
		    
  filename3="$FileName/pre_marker_marker_markerintra.txt"
  filename5="$FileName/marker_markerintra.txt"
  awk -v n=3 '{for (i=1; i<=n; i++) printf "%s%s", ($i + $(i+n) + $(i+n*2)), 
            (i==n)?ORS:OFS}' <(paste $filename1 $filename2) > $filename3
           
  cat $filename3 | awk '{printf( "%.2f\t%.2f\t%.2f\n", $1 * (-1), $2, $3 * (-1) )}' > $filename5
 
  rm -rfv $filename3
  
  #  folder for contours, dosis and shift 
  
  Folder_inter_intra="inter_intra_shift"
  
  Folder_kontur_shift="shifted_kontur_dose_data"
  Folder_shift_dose="shifted_dose_data"

  mkdir $FileName/$Folder_inter_intra
  
  mkdir $FileName/$Folder_inter_intra/$Folder_kontur_shift # erstellt folder in dem gegebenen path
  mkdir $FileName/$Folder_inter_intra/$Folder_shift_dose # erstellt folder in dem gegebenen path

  # folder for dose addition and dvh
  
  Folder1="dose_addition"
  mkdir $FileName/$Folder_inter_intra/$Folder1 # erstellt folder in dem gegebenen path
  Folder2="dvh"
  mkdir $FileName/$Folder_inter_intra/$Folder2 # erstellt folder in dem gegebenen path

  #entity1="Rektum"
  #entity2="Rektumvorderwand"


  mkdir $FileName/$Folder_inter_intra/$Folder2/$entity1 # erstellt folder in dem gegebenen path
  mkdir $FileName/$Folder_inter_intra/$Folder2/$entity2 # erstellt folder in dem gegebenen path
  mkdir $FileName/$Folder_inter_intra/$Folder2/$entity3 # erstellt folder in dem gegebenen path
  mkdir $FileName/$Folder_inter_intra/$Folder2/$entity4 # erstellt folder in dem gegebenen path
  mkdir $FileName/$Folder_inter_intra/$Folder2/$entity5 # erstellt folder in dem gegebenen path
  
  
  
else

  filename5="$FileName/marker_markerinter.txt"
  cat $filename1 | awk '{printf( "%.2f\t%.2f\t%.2f\n", $1 , $2, $3 )}' > $filename5
  
  #  folder for contours, dosis and shift 
  
  Folder_inter_intra="inter_shift"
  
  Folder_kontur_shift="shifted_kontur_dose_data"
  Folder_shift_dose="shifted_dose_data"

  mkdir $FileName/$Folder_inter_intra # erstellt folder in dem gegebenen path
    
  mkdir $FileName/$Folder_inter_intra/$Folder_kontur_shift # erstellt folder in dem gegebenen path
  mkdir $FileName/$Folder_inter_intra/$Folder_shift_dose # erstellt folder in dem gegebenen path

  # folder for dose addition and dvh
  
  Folder1="dose_addition"
  mkdir $FileName/$Folder_inter_intra/$Folder1 # erstellt folder in dem gegebenen path
  Folder2="dvh"
  mkdir $FileName/$Folder_inter_intra/$Folder2 # erstellt folder in dem gegebenen path

  #entity1="Rektum"
  #entity2="Rektumvorderwand"


  mkdir $FileName/$Folder_inter_intra/$Folder2/$entity1 # erstellt folder in dem gegebenen path
  mkdir $FileName/$Folder_inter_intra/$Folder2/$entity2 # erstellt folder in dem gegebenen path
  mkdir $FileName/$Folder_inter_intra/$Folder2/$entity3 # erstellt folder in dem gegebenen path
  mkdir $FileName/$Folder_inter_intra/$Folder2/$entity4 # erstellt folder in dem gegebenen path
  mkdir $FileName/$Folder_inter_intra/$Folder2/$entity5 # erstellt folder in dem gegebenen path
  
  
  
fi

# get dvh 0,0,0
   
x="0"
y="0"
z="0"
	
cat > $FileName/shift.txt <<EOF
#Insight Transform File V1.0
#Transform 0
Transform: TranslationTransform_double_3_3
Parameters: $x $y $z
FixedParameters:
EOF

      shifted_dose=$(echo $dose | sed "s/\.nrrd/\_$x\_$y\_$z.nrrd/g")

      ${PLASTIMATCH} warp --input $dose --output-img $shifted_dose --fixed $dose --xf $FileName/shift.txt
 
        contour=$FileName/Contours/Rektum.nrrd 
	result=$(echo ${contour} | sed "s/\.nrrd/_"fraktion_$a".mha/g" )
	result=${result##*/}
	echo $contour "no shift ..."
        echo $contour "no shift ..."
        echo $contour "no shift ..."
        echo $contour "no shift ..."
	${MULTIPLY} $dose $contour $FileName/$Folder_inter_intra/$Folder_kontur_shift/$result
		
        contour=$FileName/Contours/Rektumvorderwand.nrrd   
	result=$(echo ${contour} | sed "s/\.nrrd/_"fraktion_$a".mha/g" )
	result=${result##*/}
	echo $contour "no shift ..."
        echo $contour "no shift ..."
        echo $contour "no shift ..."
        echo $contour "no shift ..."
	${MULTIPLY} $dose $contour $FileName/$Folder_inter_intra/$Folder_kontur_shift/$result
	
	contour=$FileName/Contours/Prostate.nrrd  
	result=$(echo ${contour} | sed "s/\.nrrd/_"fraktion_$a".mha/g" )
	result=${result##*/}
	echo $contour "no shift ..."
        echo $contour "no shift ..."
        echo $contour "no shift ..."
        echo $contour "no shift ..."
	${MULTIPLY} $dose $contour $FileName/$Folder_inter_intra/$Folder_kontur_shift/$result
		
	contour=$FileName/Contours/PTV.nrrd  
	result=$(echo ${contour} | sed "s/\.nrrd/_"fraktion_$a".mha/g" )
	result=${result##*/}
	echo $contour "no shift ..."
        echo $contour "no shift ..."
        echo $contour "no shift ..."
        echo $contour "no shift ..."
	${MULTIPLY} $dose $contour $FileName/$Folder_inter_intra/$Folder_kontur_shift/$result
	${PLASTIMATCH} stats $FileName/$Folder_inter_intra/$Folder_kontur_shift/$result
	
	contour=$FileName/Contours/Bulb.nrrd  
	result=$(echo ${contour} | sed "s/\.nrrd/_"fraktion_$a".mha/g" )
	result=${result##*/}
	echo $contour "no shift ..."
        echo $contour "no shift ..."
        echo $contour "no shift ..."
        echo $contour "no shift ..."
	${MULTIPLY} $dose $contour $FileName/$Folder_inter_intra/$Folder_kontur_shift/$result
	${PLASTIMATCH} stats $FileName/$Folder_inter_intra/$Folder_kontur_shift/$result
	
	
      mv $result $FileName/$Folder_inter_intra/$Folder_kontur_shift
      mv $shifted_dose */$Folder_inter_intra/$Folder_shift_dose
    
      rm -rfv $FileName/$shifted_dose/*
		    
      rm -rfv $FileName/shift.txt


# rektum und rektumvorderwand kontur shift dose

while read -r line
do
   
    a=$(($a+1))
    
    x=$( echo ${line} | cut -f1 -d" " )
    z=$( echo ${line} | cut -f2 -d" " )
    y=$( echo ${line} | cut -f3 -d" " )
    
cat > $FileName/shift.txt <<EOF
#Insight Transform File V1.0
#Transform 0
Transform: TranslationTransform_double_3_3
Parameters: $x $y $z
FixedParameters:
EOF

    shifted_dose=$(echo $dose | sed "s/\.nrrd/\_$x\_$y\_$z.nrrd/g")
    echo $shifted_dose
    echo $shifted_dose
    echo $shifted_dose
    echo $shifted_dose
    ${PLASTIMATCH} warp --input $dose --output-img $shifted_dose --fixed $dose --xf $FileName/shift.txt

        contour=$FileName/Contours/Rektum.nrrd 
	result=$(echo ${contour} | sed "s/\.nrrd/_"fraktion_$a".mha/g" )
	result=${result##*/}
	echo $contour "rektum ..."
        echo $contour "rektum ..."
        echo $contour "rektum ..."
	${MULTIPLY} $shifted_dose $contour $FileName/$Folder_inter_intra/$Folder_kontur_shift/$result
     
        contour=$FileName/Contours/Rektumvorderwand.nrrd   
	result=$(echo ${contour} | sed "s/\.nrrd/_"fraktion_$a".mha/g" )
	result=${result##*/}
	echo $contour "rektumwand ..."
        echo $contour "rektumwand ..."
        echo $contour "rektumwand ..."
        echo $contour "rektumwand ..."
	${MULTIPLY} $shifted_dose $contour $FileName/$Folder_inter_intra/$Folder_kontur_shift/$result
     
    
    mv $result $FileName/$Folder_inter_intra/$Folder_kontur_shift
    mv $shifted_dose */$Folder_inter_intra/$Folder_shift_dose
    
    rm -rfv $FileName/$shifted_dose/*
       
done < "$filename_ballon_shift_final"

    rm -rfv $FileName/shift.txt
    a="0"

    
# prostate kontur shift dose

while read -r line
do
   
    a=$(($a+1))
    
    x=$( echo ${line} | cut -f1 -d" " )
    z=$( echo ${line} | cut -f2 -d" " )
    y=$( echo ${line} | cut -f3 -d" " )
    
cat > $FileName/shift.txt <<EOF
#Insight Transform File V1.0
#Transform 0
Transform: TranslationTransform_double_3_3
Parameters: $x $y $z
FixedParameters:
EOF

    shifted_dose=$(echo $dose | sed "s/\.nrrd/\_$x\_$y\_$z.nrrd/g")

    ${PLASTIMATCH} warp --input $dose --output-img $shifted_dose --fixed $dose --xf $FileName/shift.txt

	contour=$FileName/Contours/Prostate.nrrd  
	result=$(echo ${contour} | sed "s/\.nrrd/_"fraktion_$a".mha/g" )
	result=${result##*/}
	echo $contour "prostate ..."
        echo $contour "prostate ..."
        echo $contour "prostate ..."
        echo $contour "prostate ..."
	${MULTIPLY} $shifted_dose $contour $FileName/$Folder_inter_intra/$Folder_kontur_shift/$result
    
	contour=$FileName/Contours/PTV.nrrd  
	result=$(echo ${contour} | sed "s/\.nrrd/_"fraktion_$a".mha/g" )
	result=${result##*/}
	echo $contour "prostate ..."
        echo $contour "prostate ..."
        echo $contour "prostate ..."
        echo $contour "prostate ..."
	${MULTIPLY} $shifted_dose $contour $FileName/$Folder_inter_intra/$Folder_kontur_shift/$result
    
	
    mv $result $FileName/$Folder_inter_intra/$Folder_kontur_shift
    mv $shifted_dose */$Folder_inter_intra/$Folder_shift_dose
    
    rm -rfv $FileName/$shifted_dose/*
       
done < "$filename5"

    rm -rfv $FileName/shift.txt
    

## dosis addieren, verschiedene konturen ##

# rektum 
${PLASTIMATCH} add --output $FileName/$Folder_inter_intra/$Folder1/$entity1/dose_addition_fr_1_1.nrrd --weight "$dose_fraktionation " $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_1.mha 
${PLASTIMATCH} add --output $FileName/$Folder_inter_intra/$Folder1/$entity1/dose_addition_fr_1_2.nrrd --weight "$dose_fraktionation $dose_fraktionation" $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_1.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_2.mha
${PLASTIMATCH} add --output $FileName/$Folder_inter_intra/$Folder1/$entity1/dose_addition_fr_1_3.nrrd --weight "$dose_fraktionation $dose_fraktionation $dose_fraktionation" $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_1.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_2.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_3.mha 
${PLASTIMATCH} add --output $FileName/$Folder_inter_intra/$Folder1/$entity1/dose_addition_fr_1_4.nrrd --weight "$dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation" $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_1.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_2.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_3.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_4.mha
${PLASTIMATCH} add --output $FileName/$Folder_inter_intra/$Folder1/$entity1/dose_addition_fr_1_5.nrrd --weight "$dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation" $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_1.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_2.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_3.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_4.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_5.mha  
${PLASTIMATCH} add --output $FileName/$Folder_inter_intra/$Folder1/$entity1/dose_addition_fr_1_6.nrrd --weight "$dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation" $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_1.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_2.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_3.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_4.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_5.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_6.mha 
${PLASTIMATCH} add --output $FileName/$Folder_inter_intra/$Folder1/$entity1/dose_addition_fr_1_7.nrrd --weight "$dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation" $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_1.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_2.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_3.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_4.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_5.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_6.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_7.mha  
${PLASTIMATCH} add --output $FileName/$Folder_inter_intra/$Folder1/$entity1/dose_addition_fr_1_8.nrrd --weight "$dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation" $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_1.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_2.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_3.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_4.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_5.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_6.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_7.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_8.mha 
${PLASTIMATCH} add --output $FileName/$Folder_inter_intra/$Folder1/$entity1/dose_addition_fr_1_9.nrrd --weight "$dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation" $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_1.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_2.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_3.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_4.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_5.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_6.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_7.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_8.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_9.mha 
${PLASTIMATCH} add --output $FileName/$Folder_inter_intra/$Folder1/$entity1/dose_addition_fr_1_10.nrrd --weight "$dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation $dose_fraktionation" $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_1.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_2.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_3.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_4.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_5.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_6.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_7.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_8.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_9.mha $FileName/$Folder_inter_intra/shifted_kontur_dose_data/$entity1\_fraktion_10.mha 
