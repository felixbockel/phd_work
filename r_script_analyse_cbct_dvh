## data ##
##########

# input arguments

args<-commandArgs(TRUE)
patientID = as.character(args[1]) # name of patient: .../dvh_kontur_shift/analysis/patientID
print(patientID);

args<-commandArgs(TRUE)
motionID = as.character(args[2]) # inter oder intra: inter includes analysis of interfractional motion and intra means inter- + intrafractional motion 
print(motionID);

# create folders

dir.create(file.path(paste("/home/boeckefx/Documents/Prostate_SOP/Data/dvh_kontur_shift/analysis/",patientID, sep="")))
dir.create(file.path(paste("/home/boeckefx/Documents/Prostate_SOP/Data/dvh_kontur_shift/analysis/",patientID,"/",motionID, sep="")))
dir.create(file.path(paste("/home/boeckefx/Documents/Prostate_SOP/Data/dvh_kontur_shift/analysis/",patientID,"/",motionID, sep=""),"png_all_fr"))

# Graph parameters

color_prostate=rgb(1,0,1,1) # pink
color_rektwand=rgb(0,0.5,0,1) # dunkelgrün
color_rektum=rgb(0,1,0,1) # hellgrün
color_bulb=rgb(0,1,1,1) # hellblau
color_prostate_totaldose=rgb(1,0,1,1)
color_rektwand_totaldose=rgb(0,0.5,0,1)
color_rektum_totaldose=rgb(0,1,0,1)
color_bulb_totaldose=rgb(0,1,1,1)
type_line_nonshift="l"
type_line_shift="l"
lty_solid=1 # for non shift
lty_dotted=2 # for shift

seq_dose=seq(0,55,5)
seq_vol_name=seq(0,100,10)
seq_vol_pos=seq(0,1,0.1)
font_size=0.9
font_type=1
x_axis="Dose [Gy]"
y_axis="Volume [%]"
x_lim=c(0,55)

# print graph

#png
print_width=8
print_height=4
print_res=300

myPng <- function(..., width=print_width, height=print_height, res=print_res) {
  png(..., width=width*res, height=height*res, res=res)
  
}

# pdf
#pdf_width=9
#pdf_height=5

# print dvh verlauf png
path_print_totaldose=paste("/home/boeckefx/Documents/Prostate_SOP/Data/dvh_kontur_shift/analysis/",patientID,"/",motionID,"/totaldose.png", sep="")
path_print_doseverlauf=paste("/home/boeckefx/Documents/Prostate_SOP/Data/dvh_kontur_shift/analysis/",patientID,"/",motionID,"/png_all_fr/", sep="")

# print pdf
#path_pdf_totaldose=paste("/home/boeckefx/Documents/Prostate_SOP/Data/dvh_kontur_shift/analysis/",patientID,"/totaldose.pdf", sep="")
#path_pdf_doseverlauf=paste("/home/boeckefx/Documents/Prostate_SOP/Data/dvh_kontur_shift/analysis/",patientID,"/doseverlauf.pdf", sep="")

#
legend_totaldose=c("Prostate dose without motion","Prostate dose with motion","Anterior rectal wall dose without motion","Anterior rectal wall dose with motion","Rectum dose without motion","Rectum dose with motion","Penile Bulb without motion")
#

if(motionID == "intra"){

   pathID=paste(patientID,"/","inter_intra_shift",sep="")
   print(pathID)
   
} else {
   
   pathID=paste(patientID,"/","inter_shift",sep="")
   print(pathID)
   
}



