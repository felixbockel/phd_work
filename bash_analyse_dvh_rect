while [ $1 ]; do
    case $1 in
        "-f")        filename=$2;;
        
        "-df")       dose_fraktionation=$2;;
        
        "-ii")        inter_intra=$2;; # inter or intra
        
        "-ent1")        entity1=$2;;
        
        "-ent2")       entity2=$2;;
    esac
    shift
done

